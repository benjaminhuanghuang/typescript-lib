{
  "compilerOptions": {
    "declaration": true,
    "inlineSources": true,
    "lib": [
      "es2015"
    ],
    "outDir": "build",
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "newLine": "LF",
    "noImplicitAny": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "removeComments": false,
    "sourceMap": true,
    "strictNullChecks": true
  },
  "files": [
    "index.ts"
  ]
}